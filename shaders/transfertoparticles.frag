precision mediump float;vec3 a(vec3 b){return vec3(mod(gl_FragCoord.x,b.x),gl_FragCoord.y,floor(gl_FragCoord.x/b.x)+0.5);}vec4 c(sampler2D d,vec3 e,vec3 b){vec3 f=e*b;f=clamp(f,vec3(0.5),vec3(b-0.5));float g=floor(f.z-0.5);float h=g+1.0;float i=fract(f.z-0.5);vec2 j=vec2(g*b.x+f.x,f.y)/vec2(b.x*b.z,b.y);vec2 k=vec2(h*b.x+f.x,f.y)/vec2(b.x*b.z,b.y);return mix(texture2D(d,j),texture2D(d,k),i);}vec4 l(sampler2D d,vec3 e,vec3 b){vec3 f=e*b;f=clamp(f,vec3(0.5),vec3(b-0.5));float m=floor(f.z);vec2 n=vec2(m*b.x+f.x,f.y)/vec2(b.x*b.z,b.y);return texture2D(d,n);}varying vec2 v_coordinates;uniform sampler2D u_particlePositionTexture;uniform sampler2D u_particleVelocityTexture;uniform sampler2D u_gridVelocityTexture;uniform sampler2D u_originalGridVelocityTexture;uniform vec3 u_gridResolution;uniform vec3 u_gridSize;uniform float u_flipness;float o(sampler2D d,vec3 p){vec3 q=vec3(p.x,p.y-0.5,p.z-0.5);return c(d,(q+0.5)/(u_gridResolution+1.0),u_gridResolution+1.0).x;}float r(sampler2D d,vec3 p){vec3 q=vec3(p.x-0.5,p.y,p.z-0.5);return c(d,(q+0.5)/(u_gridResolution+1.0),u_gridResolution+1.0).y;}float s(sampler2D d,vec3 p){vec3 q=vec3(p.x-0.5,p.y-0.5,p.z);return c(d,(q+0.5)/(u_gridResolution+1.0),u_gridResolution+1.0).z;}vec3 t(sampler2D d,vec3 p){return vec3(o(d,p),r(d,p),s(d,p));}void main(){vec3 u=texture2D(u_particlePositionTexture,v_coordinates).rgb;u=(u/u_gridSize)*u_gridResolution;vec3 v=texture2D(u_particleVelocityTexture,v_coordinates).rgb;vec3 w=t(u_gridVelocityTexture,u);vec3 x=t(u_originalGridVelocityTexture,u);vec3 y=w-x;vec3 z=v+y;vec3 A=w;gl_FragColor=vec4(mix(A,z,u_flipness),0.0);}